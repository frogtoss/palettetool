# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug_x64
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_x64)
  palettetool_config = debug_x64
endif
ifeq ($(config),debug_x86)
  palettetool_config = debug_x86
endif
ifeq ($(config),release_x64)
  palettetool_config = release_x64
endif
ifeq ($(config),release_x86)
  palettetool_config = release_x86
endif

PROJECTS := palettetool

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

palettetool:
ifneq (,$(palettetool_config))
	@echo "==== Building palettetool ($(palettetool_config)) ===="
	@${MAKE} --no-print-directory -C . -f palettetool.make config=$(palettetool_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f palettetool.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_x64"
	@echo "  debug_x86"
	@echo "  release_x64"
	@echo "  release_x86"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   palettetool"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"